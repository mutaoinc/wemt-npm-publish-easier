    "publish:init": "npm-publish-easier --init",
    "publish:clean": "npm-publish-easier --clean"
  },
  "keywords": [
    "npm",
    "publish",
    "automation",
    "version",
    "package",
    "build",
    "deploy",
    "enhanced",
    "workflow",
    "cli"
  ],
  "author": "Mutaoinc & Wemt Team",
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mutaoinc/wemt-npm-publish-easier.git"
  },
  "homepage": "https://github.com/mutaoinc/wemt-npm-publish-easier#readme",
  "bugs": {
    "url": "https://github.com/mutaoinc/wemt-npm-publish-easier/issues"
  },
  "devDependencies": {
    "rollup": "^4.44.1"
  },
  "publishConfig": {
    "access": "public"
  }
}