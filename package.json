{
  "name": "@wemt/npm-publish-easier",
  "version": "1.1.11",
  "description": "An enhanced npm package publishing automation tool that supports intelligent building, version management, file copying, and one-click publishing.",
  "main": "index.js",
  "bin": {
    "npm-publish-easier": "index.js"
  },
  "scripts": {
    "publish": "npm-publish-easier -y",
